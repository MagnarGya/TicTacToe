<Window x:Class="TicTacToe.GameOver"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TicTacToe"
        mc:Ignorable="d"
        Title="Game Over" Height="100" Width="200" ResizeMode="NoResize">
    <StackPanel Orientation="Vertical">
        <TextBlock Name="Congratulations"></TextBlock>
        <Button Name="Ok" Click="ButtonOk" Width="40" HorizontalAlignment="Right">Ok</Button>
    </StackPanel>
</Window>
